{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": ["main"],
    "permissions": [
        "core:default",
        "opener:default",
        "sql:default",
        "sql:allow-execute",
        "fs:default",
        {
            "identifier": "fs:allow-read-text-file",
            "allow": [
                {
                    "path": "$APPCONFIG/testing.m3u"
                }
            ]
        }
    ]
}
